<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="walkthechat_export">
                <settings>
                    <url path="walkthechat/product/export" />
                    <type>walkthechat_export</type>
                    <label translate="true">Export To Walkthechat</label>
                </settings>
            </action>
            <action name="walkthechat_delete">
                <settings>
                    <url path="walkthechat/product/delete" />
                    <type>walkthechat_delete</type>
                    <label translate="true">Delete From Walkthechat</label>
                    <confirm>
                        <title>Delete items</title>
                        <message>Are you sure you want to delete selected products from WalkTheChat?</message>
                    </confirm>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>
