<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="context" xsi:type="configurableObject">
        <argument name="class" xsi:type="string">Magento\Framework\View\Element\UiComponent\Context</argument>
        <argument name="namespace" xsi:type="string">wtc_walkthechat_queue_listing</argument>
    </argument>
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">wtc_walkthechat_queue_listing.wtc_walkthechat_queue_grid_data_source</item>
            <item name="deps" xsi:type="string">wtc_walkthechat_queue_listing.wtc_walkthechat_queue_grid_data_source</item>
        </item>
        <item name="spinner" xsi:type="string">wtc_walkthechat_queue_grid_columns</item>
        <item name="buttons" xsi:type="array">
            <item name="delete_all" xsi:type="string">Walkthechat\Walkthechat\Block\Adminhtml\Dashboard\DeleteAllButton</item>
            <!--<item name="sync_shipping" xsi:type="array">-->
                <!--<item name="name" xsi:type="string">sync_shipping</item>-->
                <!--<item name="label" xsi:type="string" translate="true">Sync Tablerate Shipping</item>-->
                <!--<item name="class" xsi:type="string">primary</item>-->
                <!--<item name="url" xsi:type="string">*/*/syncShipping</item>-->
                <!--<item name="sort_order" xsi:type="string">100</item>-->
            <!--</item>-->
            <item name="export_all" xsi:type="array">
                <item name="name" xsi:type="string">export_all</item>
                <item name="label" xsi:type="string" translate="true">Export All Products</item>
                <item name="class" xsi:type="string">primary</item>
                <item name="url" xsi:type="string">*/product/exportAll</item>
                <item name="sort_order" xsi:type="string">200</item>
            </item>
            <item name="export_selected" xsi:type="array">
                <item name="name" xsi:type="string">export_selected</item>
                <item name="label" xsi:type="string" translate="true">Export Selected Products</item>
                <item name="class" xsi:type="string">primary</item>
                <item name="url" xsi:type="string">catalog/product</item>
                <item name="sort_order" xsi:type="string">300</item>
            </item>
        </item>
    </argument>
    <dataSource name="wtc_walkthechat_queue_grid_data_source">
        <argument name="dataProvider" xsi:type="configurableObject">
            <argument name="class" xsi:type="string">WalkthechatQueueGridDataProvider</argument>
            <argument name="name" xsi:type="string">wtc_walkthechat_queue_grid_data_source</argument>
            <argument name="primaryFieldName" xsi:type="string">entity_id</argument>
            <argument name="requestFieldName" xsi:type="string">id</argument>
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/provider</item>
                    <item name="update_url" xsi:type="url" path="mui/index/render" />
                    <item name="storageConfig" xsi:type="array">
                        <item name="indexField" xsi:type="string">entity_id</item>
                    </item>
                </item>
            </argument>
        </argument>
    </dataSource>
    <container name="listing_top">
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="template" xsi:type="string">ui/grid/toolbar</item>
                <item name="stickyTmpl" xsi:type="string">ui/grid/sticky/toolbar</item>
            </item>
        </argument>
        <bookmark name="bookmarks">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="storageConfig" xsi:type="array">
                        <item name="namespace" xsi:type="string">wtc_walkthechat_queue_listing</item>
                    </item>
                </item>
            </argument>
        </bookmark>
        <container name="columns_controls">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="columnsData" xsi:type="array">
                        <item name="provider" xsi:type="string">wtc_walkthechat_queue_listing.wtc_walkthechat_queue_listing.wtc_walkthechat_queue_grid_columns</item>
                    </item>
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/controls/columns</item>
                    <item name="displayArea" xsi:type="string">dataGridActions</item>
                </item>
            </argument>
        </container>
        <filterSearch name="fulltext">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="provider" xsi:type="string">wtc_walkthechat_queue_listing.wtc_walkthechat_queue_grid_data_source</item>
                    <item name="chipsProvider" xsi:type="string">wtc_walkthechat_queue_listing.wtc_walkthechat_queue_listing.listing_top.listing_filters_chips</item>
                    <item name="storageConfig" xsi:type="array">
                        <item name="provider" xsi:type="string">wtc_walkthechat_queue_listing.wtc_walkthechat_queue_listing.listing_top.bookmarks</item>
                        <item name="namespace" xsi:type="string">current.search</item>
                    </item>
                </item>
            </argument>
        </filterSearch>
        <filters name="listing_filters">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="columnsProvider" xsi:type="string">wtc_walkthechat_queue_listing.wtc_walkthechat_queue_listing.wtc_walkthechat_queue_grid_columns</item>
                    <item name="storageConfig" xsi:type="array">
                        <item name="provider" xsi:type="string">wtc_walkthechat_queue_listing.wtc_walkthechat_queue_listing.listing_top.bookmarks</item>
                        <item name="namespace" xsi:type="string">current.filters</item>
                    </item>
                    <item name="childDefaults" xsi:type="array">
                        <item name="provider" xsi:type="string">wtc_walkthechat_queue_listing.wtc_walkthechat_queue_listing.listing_top.listing_filters</item>
                        <item name="imports" xsi:type="array">
                            <item name="visible" xsi:type="string">wtc_walkthechat_queue_listing.wtc_walkthechat_queue_listing.wtc_walkthechat_queue_grid_columns.${ $.index }:visible</item>
                        </item>
                    </item>
                </item>
            </argument>
        </filters>
        <massaction name="listing_massaction">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="selectProvider" xsi:type="string">wtc_walkthechat_queue_listing.wtc_walkthechat_queue_listing.wtc_walkthechat_queue_grid_columns.ids</item>
                    <item name="indexField" xsi:type="string">entity_id</item>
                </item>
            </argument>
            <action name="delete">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">delete</item>
                        <item name="label" xsi:type="string" translate="true">Delete</item>
                        <item name="url" xsi:type="url" path="walkthechat/dashboard/massDelete" />
                        <item name="confirm" xsi:type="array">
                            <item name="title" xsi:type="string" translate="true">Delete items</item>
                            <item name="message" xsi:type="string" translate="true">Are you sure you wan't to delete selected items from the queue?</item>
                        </item>
                    </item>
                </argument>
            </action>
        </massaction>
        <paging name="listing_paging">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="storageConfig" xsi:type="array">
                        <item name="provider" xsi:type="string">wtc_walkthechat_queue_listing.wtc_walkthechat_queue_listing.listing_top.bookmarks</item>
                        <item name="namespace" xsi:type="string">current.paging</item>
                    </item>
                    <item name="selectProvider" xsi:type="string">wtc_walkthechat_queue_listing.wtc_walkthechat_queue_listing.contacts_grid_columns.ids</item>
                </item>
            </argument>
        </paging>
    </container>
    <columns name="wtc_walkthechat_queue_grid_columns">
        <selectionsColumn name="ids">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="indexField" xsi:type="string">entity_id</item>
                </item>
            </argument>
        </selectionsColumn>
        <column name="product_id" class="Walkthechat\Walkthechat\Ui\Component\Listing\Column\Item">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">false</item>
                    <item name="label" xsi:type="string" translate="true">Item</item>
                    <item name="sortable" xsi:type="boolean">false</item>
                </item>
            </argument>
        </column>
        <column name="action">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="label" xsi:type="string" translate="true">Action</item>
                </item>
            </argument>
        </column>
        <column name="created_at" class="Magento\Ui\Component\Listing\Columns\Date">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">dateRange</item>
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/columns/date</item>
                    <item name="dataType" xsi:type="string">date</item>
                    <item name="sorting" xsi:type="string">desc</item>
                    <item name="label" xsi:type="string" translate="true">Created At</item>
                </item>
            </argument>
        </column>
        <column name="status">
            <argument name="data" xsi:type="array">
                <item name="options" xsi:type="object">Walkthechat\Walkthechat\Model\Config\Source\QueueItemStatus</item>
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">select</item>
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/columns/select</item>
                    <item name="dataType" xsi:type="string">select</item>
                    <item name="editor" xsi:type="array">
                        <item name="editorType" xsi:type="string">select</item>
                    </item>
                    <item name="label" xsi:type="string" translate="true">Status</item>
                </item>
            </argument>
        </column>
    </columns>
    <container name="sticky">
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="component" xsi:type="string">Magento_Ui/js/grid/sticky/sticky</item>
                <item name="toolbarProvider" xsi:type="string">wtc_walkthechat_queue_listing.wtc_walkthechat_queue_listing.listing_top</item>
                <item name="listingProvider" xsi:type="string">wtc_walkthechat_queue_listing.wtc_walkthechat_queue_listing.wtc_walkthechat_queue_grid_columns</item>
            </item>
        </argument>
    </container>
</listing>
